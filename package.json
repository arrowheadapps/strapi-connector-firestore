{
  "name": "strapi-connector-firestore",
  "version": "3.0.0-alpha.10",
  "description": "Strapi database connector for Firestore database on Google Cloud Platform.",
  "keywords": [
    "firestore",
    "hook",
    "orm",
    "nosql",
    "strapi"
  ],
  "author": "Arrowhead Apps Ltd",
  "license": "MIT",
  "repository": "github:arrowheadapps/strapi-connector-firestore",
  "main": "lib/index.js",
  "engines": {
    "node": ">=12.0.0",
    "npm": ">=6.0.0"
  },
  "files": [
    "lib/**"
  ],
  "scripts": {
    "build": "rm -rf lib && tsc",
    "prepare": "npm run snyk-protect && tsc --skipLibCheck",
    "test": "npm run build && npm test --prefix test",
    "combine": "cd input && istanbul-combine -d ../coverage -p detail -r json **/*.json",
    "snyk-protect": "snyk protect"
  },
  "dependencies": {
    "@google-cloud/firestore": "^4.1.1",
    "lodash": "^4.17.19",
    "strapi-utils": "^3.0.6",
    "snyk": "^1.364.2"
  },
  "devDependencies": {
    "@types/lodash": "^4.14.157",
    "@types/node": "^14.0.23",
    "@types/pino": "^6.3.0",
    "typescript": "^3.9.7"
  },
  "snyk": true
}
