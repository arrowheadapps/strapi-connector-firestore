{
  "private": true,
  "scripts": {
    "postinstall": "patch-package",
    "pretest": "node -e \"require('./helpers/testAppGenerator').setupTestApp()\"",
    "posttest": "node -e \"require('./helpers/testAppGenerator').cleanTestApp()\"",
    "test": "jest --config=jest.config.e2e.js --runInBand --forceExit --detectOpenHandles --json --outputFile=../coverage/results.json",
    "coverage-remap": "remap-istanbul --input ../coverage/coverage-final.json --output ../coverage/coverage-mapped.json --basePath strapi-connector-firestore",
    "coverage-combine": "mkdir -p ../coverage/combined && istanbul-combine -d ../coverage/combined -p detail -r json ../coverage/*/coverage-mapped.json"
  },
  "dependencies": {
    "execa": "^5.0.0",
    "firebase-tools": "^8.17.0",
    "fs-extra": "^9.0.1",
    "glob": "^7.1.6",
    "glob-promise": "^3.4.0",
    "istanbul-combine": "^0.3.0",
    "jest": "^26.6.3",
    "patch-package": "^6.2.2",
    "remap-istanbul": "^0.13.0",
    "request-promise-native": "^1.0.9",
    "rimraf": "^3.0.2",
    "strapi": "^3.3.4",
    "strapi-admin": "^3.3.4",
    "strapi-connector-firestore": "file:..",
    "strapi-plugin-content-manager": "^3.3.4",
    "strapi-plugin-content-type-builder": "^3.3.4",
    "strapi-plugin-upload": "^3.3.4",
    "strapi-plugin-users-permissions": "^3.3.4",
    "strapi-utils": "^3.3.4",
    "wait-on": "^5.2.0"
  }
}
